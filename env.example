# Main domain
MAIN_DOMAIN=yourdomain.com

# Timezone
TZ=Europe/Madrid

# Cloudflare Tunnel
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

# Cloudflare API (for ACME DNS challenge)
CF_API_EMAIL=your_cloudflare_email@example.com
CF_API_KEY=your_cloudflare_api_key_here
CF_DNS_API_TOKEN=your_cloudflare_dns_api_token_here

# ACME Email for Let's Encrypt certificates
ACME_EMAIL=your_email@example.com

# Traefik Configuration
TRAEFIK_LOG_LEVEL=INFO
TRAEFIK_HTTP_PORT=80
TRAEFIK_HTTPS_PORT=443
TRAEFIK_DASHBOARD_ENABLED=true
TRAEFIK_API_INSECURE=false

# Traefik Basic Auth (generate with: htpasswd -nb admin password)
# Format: user:hashed_password (multiple users separated by commas)
TRAEFIK_BASIC_AUTH=admin:$$apr1$$hashed_password_here

# AdGuard Home Configuration
ADGUARD_DNS_IP=192.168.0.100
ADGUARD_HTTP_PORT=8050
ADGUARD_HTTPS_PORT=4430
ADGUARD_ADMIN_PORT=3000
ADGUARD_DOT_PORT=853
ADGUARD_DOH_PORT=784
ADGUARD_QUIC_PORT=8853
ADGUARD_TLS_PORT=5443
ADGUARD_DHCP_SERVER_PORT=67
ADGUARD_DHCP_CLIENT_PORT=68
